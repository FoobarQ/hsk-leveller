{
	"name": "HSK Leveller",
	"description": "Levels hsk stuff or whatever the fuck",
	"version": "0.5.1",
	"manifest_version": 3,
	"background": {
		"service_worker": "background.js"
	},
	"permissions": ["activeTab", "scripting", "tabs"],
	"web_accessible_resources": [
		{
			"resources": [
				"levels/1.txt",
				"levels/2.txt",
				"levels/3.txt",
				"levels/4.txt",
				"levels/5.txt",
				"levels/6.txt",
				"levels/7+.txt"
			],
			"matches": ["<all_urls>"]
		}
	],
	"action": {
		"default_title": "HSK Leveller"
	}
}
